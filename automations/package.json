{
  "name": "erxes-automations",
  "version": "0.11.2",
  "description": "erxes automations",
  "homepage": "https://erxes.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/erxes/erxes-automations"
  },
  "bugs": "https://github.com/erxes/erxes-automations/issues",
  "keywords": [
    "node",
    "express",
    "graphql",
    "apollo"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node dist",
    "dev": "DEBUG=erxes-automations:* node_modules/.bin/ts-node-dev --respawn src",
    "test": "NODE_ENV=test jest --forceExit",
    "build": "tsc -p tsconfig.prod.json",
    "lint": "tslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "precommit": "lint-staged",
    "migrate": "NODE_ENV=command migrate --migrations-dir='./dist/migrations' --store='./db-migrate-store.js' up",
    "customCommand": "ts-node ./src/commands/customCommand.ts",
    "resetMigrations": "ts-node ./src/commands/resetMigrations.ts",
    "release": "release-it"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@google-cloud/pubsub": "^1.1.5",
    "amqplib": "^0.5.5",
    "body-parser": "^1.17.1",
    "botbuilder": "^4.4.0",
    "botbuilder-adapter-facebook-erxes": "^1.0.5",
    "botkit": "^4.0.2",
    "crypto": "^1.0.1",
    "debug": "^4.1.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.4",
    "fbgraph": "^1.4.4",
    "formidable": "^1.2.1",
    "googleapis": "^40.0.0",
    "meteor-random": "^0.0.3",
    "requestify": "^0.2.5",
    "migrate": "^1.6.2",
    "mongoose": "5.7.5",
    "nylas": "^4.7.0",
    "querystring": "^0.2.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "sanitize-html": "^1.20.1",
    "sinon": "^7.5.0",
    "uuid": "^3.3.3",
    "apollo-server-express": "^2.9.7",
    "cors": "^2.8.1",
    "redis": "^2.8.0"
  },
  "peerOptionalDependencies": {
    "kerberos": "^1.0.0"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^1.1.0",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.0",
    "@types/mongodb": "^3.1.2",
    "@types/mongoose": "^5.2.1",
    "@types/node": "^10.12.18",
    "@types/q": "^1.5.0",
    "faker": "^4.1.0",
    "husky": "^0.13.4",
    "jest": "22.0.4",
    "lint-staged": "^3.6.0",
    "prettier": "^1.19.1",
    "release-it": "^12.4.3",
    "ts-jest": "22.0.4",
    "ts-node": "^7.0.0",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.1.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^3.7.2"
  },
  "snyk": true
}
