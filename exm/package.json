{
  "name": "exm",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "linters": {
      "modules/**/*.{ts,tsx,json,css}": [
        "prettier --single-quote --write",
        "git add"
      ],
      "pages/**/*.ts": [
        "prettier --single-quote --write",
        "git add"
      ]
    }
  },
  "scripts": {
    "dev": "node server.js",
    "build": "next build && next export",
    "start": "next start",
    "format": "prettier --single-quote --write '@(pages)/**/*.ts' && prettier --single-quote --write '@(modules)/**/*.@(ts|tsx|json|css)'",
    "precommit": "lint-staged",
    "lint": "func () { if [ -z \"$1\" ]; then tslint '@(pages|modules)/**/*.ts?(x)'; else echo \"${1}/**/*.ts?(x)\" && tslint ${1}'/**/*.ts?(x)'; fi ;}; func"
  },
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@nateradebaugh/react-datetime": "4.0.0-rc.10",
    "@types/next": "^9.0.0",
    "audio-recorder-polyfill": "^0.4.1",
    "classnames": "^2.3.2",
    "color": "^3.1.3",
    "date-fns": "1.30.1",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "draft-js": "^0.11.7",
    "erxes-icon": "^1.2.1",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "graphql-ws": "^5.11.2",
    "i18n-react": "^0.7.0",
    "lodash": "^4.17.21",
    "lodash.flowright": "^3.5.0",
    "next": "10.0.5",
    "next-routes": "^1.4.2",
    "query-string": "^5.0.0",
    "react": "17.0.1",
    "react-bootstrap": "1.0.0-beta.14",
    "react-color": "^2.17.3",
    "react-dom": "17.0.1",
    "react-facebook-login": "^4.1.1",
    "react-intersection-observer": "^9.5.2",
    "react-scrollspy": "^3.4.3",
    "react-select-plus": "^1.0.0-rc.5",
