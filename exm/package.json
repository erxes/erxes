    "babel-plugin-module-resolver": "^4.1.0",
    "detect-browser": "^5.2.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
