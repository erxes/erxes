        "git add"
      ]
    }
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/erxes",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  },
  "scripts": {
    "format": "prettier --single-quote --write 'src/**/*.@(ts|tsx)'",
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "build": "yarn generateVersion && react-scripts build",
    "compress": "gzip -5 --keep -r build/static/js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "tslint 'src/**/*.@(ts|tsx)'",
    "postinstall": "opencollective postinstall",
    "generateVersion": "node commands/generateVersion.js",
    "release": "release-it",
    "componentTest": "jest --runInBand --forceExit --detectOpenHandles --coverage",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
