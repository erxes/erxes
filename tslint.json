{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "variable-name": [
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "member-access": false,
    "no-angle-bracket-type-assertion": false,
    "max-classes-per-file": [
      true,
      5
    ],
    "interface-over-type-literal": false
  }
}