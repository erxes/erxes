{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "declaration": false,
    "target": "ES2019",
    "moduleResolution": "node",
    "module": "commonjs",
    // "lib": ["es2019", "es6", "es7", "esnext.asynciterable"],
    "noImplicitAny": false,
    "skipLibCheck": true,
    "strict": true,
    "useUnknownInCatchVariables": false,
    "preserveSymlinks": true,
    "allowSyntheticDefaultImports": true
  }
}
