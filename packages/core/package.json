{
  "name": "core",
  "version": "1.0.0",
  "keywords": [
    "node",
    "express",
    "graphql",
    "apollo"
  ],
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "start": "node dist",
    "start-crons": "node dist/cronJobs",
    "start-workers": "node dist/workers",
    "dev": "ERXES_TELEMETRY_DISABLED=1 DEBUG=erxes* NODE_ENV=development nodemon src/index.ts",
    "dev-crons": "PROCESS_NAME=crons DEBUG=erxes-crons:* NODE_ENV=development nodemon src/cronJobs",
    "loadTestData": "ts-node ./src/commands/loadTestData.ts",
    "build": "tsc -p tsconfig.prod.json && tsc-alias -p tsconfig.prod.json && node fix-build.js",
    "test": "node --expose-gc --max_old_space_size=8000 ./node_modules/.bin/jest --forceExit --silent --logHeapUsage --w=1",
    "customCommand": "node_modules/.bin/ts-node src/commands/customCommand.ts",
    "migrateBoardItems": "node_modules/.bin/ts-node src/commands/migrateBoardItems.ts",
    "createSystemFieldsGroups": "node_modules/.bin/ts-node src/commands/createSystemFieldsGroups.ts",
    "addMiddleName": "node_modules/.bin/ts-node src/commands/addMiddleName.ts",
    "removeUnusedFields": "node_modules/.bin/ts-node src/commands/removeUnusedFields.ts",
    "migrateKnowledgeBase": "node_modules/.bin/ts-node src/commands/migrateKnowledgeBase.ts",
    "migrateSegment": "node_modules/.bin/ts-node src/commands/segmentMigration.ts",
    "changeToSubscribed": "node_modules/.bin/ts-node src/commands/changeToSubscribed.ts",
    "fixCompanyScopeBrandIds": "node_modules/.bin/ts-node src/commands/fixCompanyScopeBrandIds.ts",
    "migrateContentType": "node_modules/.bin/ts-node src/commands/migrateContentType.ts",
    "migrateLogs": "node_modules/.bin/ts-node src/commands/migrateLogs.ts",
    "migrateFieldsGroups": "node_modules/.bin/ts-node src/commands/migrateFieldsGroups.ts",
    "migrateApps": "node_modules/.bin/ts-node src/commands/migrateApps.ts",
    "migrateDirections": "node_modules/.bin/ts-node src/commands/migrateDirections.ts",
    "migrateRoutes": "node_modules/.bin/ts-node src/commands/migrateRoutes.ts"
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.5.1",
    "aws-sdk": "^2.151.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.3",
    "csv-parser": "^3.0.0",
    "erxes-inmemory-storage": "^1.0.20",
    "faker": "^4.1.0",
    "file-type": "^10.4.0",
    "firebase-admin": "^8.6.1",
    "formidable": "1.2.6",
    "handlebars": "^4.7.3",
    "helmet": "^3.23.3",
    "json2csv": "^5.0.1",
    "node-schedule": "^1.2.5",
    "nodemailer": "^4.1.3",
    "server-timing-header": "^1.9.0",
    "sha256": "^0.2.0",
    "stream-array": "^1.1.2",
    "ts-node": "^10.7.0",
    "validator": "^9.0.0",
    "xlsx-populate": "^1.20.1",
    "fs-extra": "^9.0.1"
  }
}
